pipeline {
    agent any
    stages {
	    stage ('Upload to AWS') {
		    steps {
			    withAWS(region: 'ap-southeast-1', credentials: 'aws-static') {
                    s3Upload(pathStyleAccessEnabled: true, payloadSigningEnabled: true, file: 'index.html', bucket: 'jenkins-devops-pipeline')
				}
			}
		}
	}
}
